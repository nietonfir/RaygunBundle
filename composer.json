{
    "name": "nietonfir/raygun-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for integrating mindscape/raygun4php",
    "keywords": [
        "raygun",
        "errors",
        "exceptions",
        "logging"
    ],
    "homepage": "https://github.com/Nietonfir/RaygunBundle",
    "license": "MIT",
    "authors": [
        {
            "name"    : "nietonfir",
            "email"   : "nietonfir@gmail.com",
            "homepage": "https://nietonfir.eu",
            "role"    : "developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/config": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "monolog/monolog": "~1.8",
        "twig/twig": "~1.23|~2.0",
        "mindscape/raygun4php": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "symfony/yaml": "~2.3|~3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Nietonfir\\RaygunBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Monolog\\" : "vendor/monolog/monolog/tests/Monolog/",
            "Nietonfir\\RaygunBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
